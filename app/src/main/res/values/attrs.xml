<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JournalView">
        <attr name="default_paper_height" format="dimension|reference" />
        <attr name="frame_height" format="dimension|reference" />
        <attr name="frame_color" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="MusicCoverView">
        <attr name="shape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
        <attr name="radius" format="integer" />
        <attr name="trackColor" format="color" />
    </declare-styleable>
</resources>